{
  "name": "nyanpass",
  "version": "0.0.1",
  "description": "nyanpass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kdama/nyanpass.git"
  },
  "scripts": {
    "start": "npm-run-all -l clean -p serve watch",
    "test": "run-s test:clean test:single",
    "build": "webpack",
    "clean": "del-cli ./build",
    "lint": "run-p lint:*",
    "fix": "prettier -l --print-width 80 --trailing-comma all --write \"*.{ts,tsx}\" \"src/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "serve": "webpack-dev-server --inline --port 8000",
    "sloc": "sloc ./src",
    "watch": "run-s test:clean test:run",
    "lint:prettier": "prettier -l --print-width 80 --trailing-comma all \"*.{ts,tsx}\" \"src/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "lint:tslint": "tslint \"./**/*.ts?(x)\" -e \"./node_modules/**\" -t stylish",
    "test:clean": "del-cli ./build/test",
    "test:run": "jest -c ./jest.config.json --watch",
    "test:single": "jest -c ./jest.config.json --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^2.8.2",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.10",
    "@types/react": "^15.0.37",
    "@types/react-dom": "^15.5.1",
    "@types/react-test-renderer": "^15.5.1",
    "@types/shelljs": "0.7.2",
    "@types/webpack": "^3.0.4",
    "awesome-typescript-loader": "^3.2.1",
    "babel-polyfill": "6.23.0",
    "chokidar-cli": "1.2.0",
    "classnames": "2.2.5",
    "cpx": "1.5.0",
    "cross-env": "^5.0.1",
    "del-cli": "^1.1.0",
    "enzyme": "^2.9.1",
    "es5to3-webpack-plugin": "1.0.0",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "0.7.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jest": "^20.0.4",
    "json-loader": "0.5.4",
    "npm-run-all": "4.0.2",
    "preact": "^8.2.0",
    "preact-compat": "3.16.0",
    "prettier": "1.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "remap-istanbul": "0.9.5",
    "sloc": "0.2.0",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.0.0",
    "typescript": "2.4.1",
    "watch": "1.0.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-espower-loader": "^1.0.2",
    "whatwg-fetch": "2.0.3"
  },
  "engines": {
    "node": ">=6"
  }
}
